{
  "name": "@testh/testh",
  "version": "0.0.1",
  "description": "Runner for the Testh project",
  "type": "commonjs",
  "prettier": "@testh/prettier-config",
  "devDependencies": {
    "@testh/prettier-config": "^0.0.1",
    "@microsoft/tsdoc-config": "^0.16.1",
    "@types/command-line-args": "^5.2.0",
    "@types/deasync": "^0.1.2",
    "@types/glob": "^7.2.0",
    "@types/jest": "~27.5",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "~18",
    "@types/selenium-webdriver": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "~5.46",
    "@typescript-eslint/parser": "~5.46",
    "eslint": "~8.16",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~26.2",
    "jest": "~28.1",
    "prettier": "~2.6",
    "rimraf": "~3.0",
    "ts-jest": "~28.0",
    "typescript": "^5.0.0"
  },
  "scripts": {
    "start": "node build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "yarn run clean && yarn run lint && yarn run prettier",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "tsc -p tsconfig.release.json",
    "lint": "eslint . --ext .ts --ext .mts",
    "test": "jest --coverage",
    "prettier": "prettier --write .",
    "test:watch": "jest --watch"
  },
  "author": "Dmytro Bondarchuk <dbondarchuk@users.noreply.github.com>",
  "dependencies": {
    "@testh/sdk": "0.0.1",
    "@testh/actions-alerts": "0.0.1",
    "@testh/actions-browser": "0.0.1",
    "@testh/actions-checks": "0.0.1",
    "@testh/actions-elements": "0.0.1",
    "@testh/actions-file": "0.0.1",
    "@testh/actions-http": "0.0.1",
    "@testh/actions-image": "0.0.1",
    "@testh/actions-navigation": "0.0.1",
    "@testh/actions-sequence": "0.0.1",
    "@testh/actions-steps": "0.0.1",
    "@testh/actions-variables": "0.0.1",
    "@testh/actions-waits": "0.0.1",
    "class-transformer": "^0.5.1",
    "command-line-args": "^5.2.1",
    "moment": "^2.29.4",
    "reflect-metadata": "^0.1.13",
    "selenium-webdriver": "^4.4.0",
    "tslib": "~2.4",
    "tsyringe": "^4.7.0",
    "yaml-js-include": "^1.0.0"
  }
}
