{
  "$schema": "https://typedoc.org/schema.json",
  "cleanOutputDir": true,
  "exclude": [
    "src/runners/runners.ts",
    "src/**/index.ts",
    "**/src/test/*",
    "**/*.test.ts",
    "**/__tests__/*",
    "**/__mocks__/*"
  ],
  "out": "./docs",
  "includeVersion": true,
  "name": "Testh Runner",
  "theme": "markdown",
  "entryPoints": [
    "src/actions",
    "src/extensions",
    "src/helpers",
    "src/services"
  ],
  "entryPointStrategy": "expand"
}