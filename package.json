{
  "name": "testh",
  "private": true,
  "workspaces": [
    "shared/prettier-config",
    "packages/sdk*",
    "packages/actions/*",
    "packages/testh"
  ],
  "scripts": {
    "build": "workspaces-foreach -pt yarn build",
    "start:no-build": "yarn workspace @testh/testh start",
    "start": "yarn build && yarn start:no-build",
    "docs": "typedoc"
  },
  "devDependencies": {
    "@microsoft/tsdoc-config": "^0.16.1",
    "@testh/prettier-config": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "~5.46",
    "@typescript-eslint/parser": "~5.46",
    "eslint": "~8.16",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~26.2",
    "jest": "~28.1",
    "prettier": "~2.6",
    "rimraf": "~3.0",
    "ts-jest": "~28.0",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.3",
    "typescript": "^5.0.0",
    "yarn-workspaces-foreach": "^0.2.0"
  }
}
